                                    <template slot="media">
                                        <img class="responsive-image" src="http://2018.igem.org/wiki/images/thumb/b/bd/T--Thessaloniki--larisa-visit-1.jpg/800px-T--Thessaloniki--larisa-visit-1.jpg"
                                            alt="Presentation at High School Students image">
                                    </template>
                                    <template slot="content">
                                        Larisa - 3/3/2018
                                    </template>
                                    <template slot="modal-content">
                                        <div class="row justify-center">
                                            <q-carousel class="col-md-6 col-sm-12 text-white" color="white"
                                                autoplay quick-nav infinite>
                                                <q-carousel-slide v-for="image in ['http://2018.igem.org/wiki/images/b/bd/T--Thessaloniki--larisa-visit-1.jpg', 'http://2018.igem.org/wiki/images/3/3c/T--Thessaloniki--larisa-visit-2.jpg', 'http://2018.igem.org/wiki/images/3/3f/T--Thessaloniki--larisa-visit-3.jpg']"
