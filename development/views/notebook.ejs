                    <div class="col-xs-12 col-sm-6 col-md-4" v-for="month in months" :key="month.title">
                        <month-table :title="month.title" :start="month.start" :length="month.length"
                            :events="events[month.key]" :filter="selectedTags" @set-events="evts => eventsToDisplay = evts"
                            @select="data => selected = data" :selected="selected">
                        </month-table>
                    </div>
                </div>
            </div>
            <div v-if="$q.platform.is.mobile" class="row justify-center">
                <q-carousel arrows quick-nav>
                    <q-carousel-slide v-for="month in months" :key="month.title" style="height: 330px">
                        <month-table :title="month.title" :start="month.start" :length="month.length"
                            :events="events[month.key]" :filter="selectedTags" @set-events="evts => eventsToDisplay = evts"
                            @select="data => selected = data" :selected="selected">
                        </month-table>
                    </q-carousel-slide>
                </q-carousel>
            </div>

            <div class="row reverse">
                <q-btn ref="clearBtn" v-show="eventsToDisplay.length > 0" class="bg-indigo-12 text-white"
                    label="Clear Selected Date" icon="clear" @click="eventsToDisplay = []; selected = {}">
                </q-btn>
            </div>
            <div ref="timeline" class="row justify-center">
                <h4 v-show="Object.keys(selected).length > 0">
                    {{iGEMParseDate(selected.pos + 1) + ' of ' + selected.month }}
