                <h3>Notebook</h3>
            </div>
            <div class="row justify-center">
                <div class="col-xs-12 col-md-6 col-lg-4">
                    <q-field class="full-width">
                        <q-select float-label="Filter Tags" multiple chips clearable v-model="selectedTags"
                            :options="tags" :dark="nightMode">
                        </q-select>
                    </q-field>
                </div>
            </div>
            <div v-if="$q.platform.is.desktop" class="row justify-center items-center">
